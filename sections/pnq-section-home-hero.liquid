      "label": "Subtitle"
    },
    {
      "type": "text",
      "id": "buttonText",
      "default": "GET STARTED",
      "label": "Button Text"
    },
    {
      "type": "url",
      "id": "buttonUrl",
      "label": "Button URL"
    },
    {
      "type": "url",
      "id": "reviewsUrl",
      "label": "Reviews URL"
    }
  ],
  "blocks": [
    {
      "type": "hero-blocks",
      "name": "Product Images",
      "limit": 4,
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Product Image"
        }
      ]
    }
  ],
    "presets": [
      {
        "name": "PNQ Home Hero",
        "category": "Pinaq"
      }
    ]
}
