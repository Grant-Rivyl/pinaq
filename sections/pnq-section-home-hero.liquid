      <a href="{{ section.settings.reviewsUrl }}" class="button-reviews w-button">REVIEWS</a>
      <div class="section-home-hero">
        <div class="panel-white">
          <div class="wrap-hero-heading">
            <div class="block-hero-heading">
              <div class="page-padding">
                <div class="padding-vertical">
                  <div class="box-hero-heading">
                    <h1 class="h1-home">{{ section.settings.title }}</h1>
                    <div class="text-size-regular">{{ section.settings.subtitle }}</div>
                    <div class="wrap-button-standard">
                      <a href="{{ section.settings.buttonUrl }}" class="button-standard w-button">{{ section.settings.buttonText }}</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
           
          </div>
          <div class="wrap-pill">
            <div class="shape-pill-out"></div>
            <div class="shape-pill-in"></div>
          </div>
        </div>
        <div class="wrap-hero-pineapples">
          <div id="w-node-_01ad3dae-4fc2-cad6-795b-41773122aed4-24e4fe69" class="page-padding">
            <div class="grid-hero-pineapples">
                {% for block in section.blocks %}
                    <img src="{{ block.settings.image | img_url: '340x' }}" loading="lazy" id="w-node-f4a4dfe8-0b5b-7cd4-f2a1-0ea501fd848f-24e4fe69" alt="" class="pineapple-blue">
                {% endfor %}
            </div>
          </div>
        </div>
